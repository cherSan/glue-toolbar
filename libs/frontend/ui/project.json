{
  "name": "frontend-ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/frontend/ui/src",
  "prefix": "ui",
  "build": {
    "options": {
      "assets": [
        {
          "input": "libs/frontend/ui/src/styles",
          "glob": "**/*",
          "output": "styles"
        },
        {
          "input": "libs/frontend/ui/src/assets",
          "glob": "**/*",
          "output": "assets"
        }
      ]
    }
  },
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/frontend/ui/**/*.ts",
          "libs/frontend/ui/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
